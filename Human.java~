public class Human{

//attributes
	private boolean gender;
	private int name;
	private int health;
	private boolean isAlive;

//constructive
	public Human(){
		this.health = 100;
		this.isAlive = true;
}
	public Human(boolean gender, int name, int health){
		this.gender = gender;
		this.name = name;
		this.health = health;
		this.isAlive = true;
}
//mutators
	public int getName(){
		return this.name;
}
	public void setName(int id){
		this.name = name;
}
//other mutators

//toString
public string toString(){
return "Human name: " + this.id + " health: " + this.health + " isAlive: " +this.isAlive
 + " gender: " + this.gender ;

}
				}
